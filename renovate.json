{
  "extends": ["github>numerique-gouv/renovate-configuration"],
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "packageRules": [
    {
      "enabled": false,
      "groupName": "ignored python dependencies",
      "matchManagers": ["pep621"],
      "matchPackageNames": []
    },
    {
      "groupName": "Ignore express folder",
      "ignorePaths": ["**/express/**"],
      "matchManagers": ["npm"]
    }
  ]
}
